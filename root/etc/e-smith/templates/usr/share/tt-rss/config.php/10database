{

use NethServer::Password;
my $password = NethServer::Password::store('tt-rss') || die('Could not generate tt-rss password!');

$OUT .=<<"HERE";

        define('DB_TYPE', "mysql");
        define('DB_HOST', "localhost");
        define('DB_PORT', "3306");
        define('DB_USER', "tt-rssuser");
        define('DB_NAME', "tt-rssdb");
        define('DB_PASS', "$password");

HERE
}

        define('MYSQL_CHARSET', 'UTF8');
        // Connection charset for MySQL.

